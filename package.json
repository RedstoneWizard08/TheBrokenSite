{
    "name": "thebrokensite",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev": "astro dev",
        "build": "astro build",
        "preview": "astro preview",
        "astro": "astro",
        "fmt": "deno fmt && prettier --write ."
    },
    "dependencies": {
        "astro": "^5.5.4",
        "typerinth": "^1.1.0"
    },
    "devDependencies": {
        "@deno/astro-adapter": "^0.3.0",
        "@unocss/preset-web-fonts": "^66.1.0-beta.6",
        "@unocss/reset": "^66.1.0-beta.6",
        "prettier": "^3.5.3",
        "prettier-plugin-astro": "^0.14.1",
        "unocss": "^66.1.0-beta.6"
    },
    "prettier": {
        "tabWidth": 4,
        "plugins": [
            "prettier-plugin-astro"
        ],
        "overrides": [
            {
                "files": "*.astro",
                "options": {
                    "parser": "astro"
                }
            }
        ]
    }
}
